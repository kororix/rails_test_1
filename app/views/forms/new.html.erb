<%= form_with(model: Form.new, local: true) do |form| %>
  <div class="form_title">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div class="form_title">
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>
  <div class="form_title">
    <%= form.label :content %>
    <%= form.text_field :content %>
  </div>
  <%= form.submit %>
<% end %>
